<script>
    $(document).on('change', '#show_state_id', function () {
        var state_id = $(this).val();
        $('select[name="city"]').empty()
        $.ajax({
            type: "POST",
            url: "https://www.shekhawatilosal.com/get-city-data",
            data: {
                "_token": "DEcAMSyYSnsTMUBTSwExPhCaA5jBvNlkPhYsvPq9",
                "state_id": state_id
            },
            dataType: "json",
            success: function (response) {

                console.log(response);
                $('select[name="city"]').append('<option value=""> Select City </option>');
                $.each(response, function (key, value) {
                    $('select[name="city"]').append('<option value="' + value.id + '">' +
                        value.name + '</option>');

                });
            }
        });

    });
</script>
 
  
